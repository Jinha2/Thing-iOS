platform :ios, '12.0'
inhibit_all_warnings!

target 'thing' do
  use_frameworks!
 
  # Network
  pod 'Alamofire'
  pod 'Moya'
  pod 'Kingfisher'
  
  # View
  pod 'NVActivityIndicatorView'
  
  # GA
  pod 'Firebase/Analytics'
  # Login
  pod 'Firebase/Auth'
  pod 'GoogleSignIn'
  # Crashlityics
  pod 'Fabric', '~> 1.9.0'
  pod 'Crashlytics', '~> 3.12.0'
end

target 'thingTests' do
  use_frameworks!

  pod 'Firebase/Analytics'
end
